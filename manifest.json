{
    "manifest_version": 3,
    "name": "YouTube Helper",
    "version": "1.0",
    "description": "Custom YouTube extension",
    "permissions": [
      "scripting"
    ],
    "host_permissions": [
      "https://*.youtube.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "64": "block.jpg"
    }
  }